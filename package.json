{
  "name": "auto-discord-developer-badge",
  "version": "1.0.0",
  "description": "Automatically maintain Discord Active Developer Badge eligibility",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/start.js",
    "start:bot": "node src/index.js",
    "start:dashboard": "node src/dashboard.js",
    "start:all": "npm run generate-guide && node src/start-all.js",
    "start:pm2": "npm run generate-guide && pm2 start ecosystem.config.cjs",
    "register": "node src/register-commands.js",
    "dev": "node --watch src/index.js",
    "generate-guide": "node src/generate-guide.js",
    "postinstall": "node src/setup.js"
  },
  "keywords": [
    "discord",
    "bot",
    "active-developer"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiSZ/DiscordBot.git"
  },
  "homepage": "https://github.com/XiSZ/DiscordBot",
  "dependencies": {
    "@iamtraction/google-translate": "^2.0.1",
    "@railway/cli": "^4.12.0",
    "axios": "^1.13.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "emoji-regex": "^10.6.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "session-file-store": "^1.5.0"
  }
}
